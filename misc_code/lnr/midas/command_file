define/local xcol/c/1/15 {p2}
define/local ycol/c/1/15 {p3}
define/local excol/c/1/15 {p4}
define/local eycol/c/1/15 {p5}
define/local exycol/c/1/15 {p6}
define/local nsim/i/1/1 {p7}
